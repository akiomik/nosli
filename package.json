{
  "name": "nosli",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --no-tsconfig",
    "check:watch": "svelte-kit sync && svelte-check --no-tsconfig --watch",
    "test": "npm run test:unit",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "favicongen": "qlmanage -t -s 512 -o static static/favicon.svg && mv static/favicon.svg.png static/favicon.png",
    "touchicongen": "qlmanage -t -s 180 -o static static/favicon.svg && mv static/favicon.svg.png static/apple-touch-icon.png"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^0.132.5",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.15.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/sanitize-html": "^2.8.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/svelte-fontawesome": "^0.2.0",
    "@macfja/svelte-persistent-store": "^2.2.1",
    "linkify-html": "^4.1.0",
    "linkify-plugin-hashtag": "^4.1.0",
    "linkify-plugin-mention": "^4.1.0",
    "linkifyjs": "^4.1.0",
    "nostr-tools": "1.6.1",
    "sanitize-html": "^2.10.0"
  }
}
